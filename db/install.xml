<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/tad/db" VERSION="202103051" COMMENT="XMLDB file for local_banner"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <!-- TADok általános adatai -->
    <TABLE NAME="tad" COMMENT="basic tad data">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="name" TYPE="text" NOTNULL="true"/>
        <FIELD NAME="author" TYPE="text" NOTNULL="true"/>
        <FIELD NAME="editable" TYPE="text" NOTNULL="true"/>
        <FIELD NAME="coursecode" TYPE="text" NOTNULL="true"/>
        <FIELD NAME="dlurl" TYPE="text" NOTNULL="true"/>
        <FIELD NAME="semester" TYPE="int" LENGTH="10" NOTNULL="true"/>
        <FIELD NAME="timerelevant" TYPE="text" NOTNULL="false"/>
        <FIELD NAME="timecreated" TYPE="text" NOTNULL="true"/>
        <FIELD NAME="version" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="1"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

    <!-- Corriculum data -->
    <TABLE NAME="tad_corriculum" COMMENT="corriculum data">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="code" TYPE="text" NOTNULL="true"/>
        <FIELD NAME="name" TYPE="text" NOTNULL="true"/>
        <FIELD NAME="coursename" TYPE="text" NOTNULL="true"/>
        <FIELD NAME="coursecode" TYPE="text" NOTNULL="true"/>
        <FIELD NAME="type" TYPE="text" NOTNULL="true"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>


    <!-- Custom TADs -->
    <TABLE NAME="tad_section_1" COMMENT="1st section of a TAD">
      <FIELDS>
        <FIELD NAME="id"                  TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="coursename"          TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="coursename_en"       TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="coursecode"          TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="coursetype"          TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="coursetype_en"       TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="lecture"             TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="practice"            TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="laboratory"          TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="assesmenttype"       TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="assesmenttype_en"    TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="credit"              TYPE="int" LENGTH="2" NOTNULL="true"               />
        <FIELD NAME="courseleadername"    TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="courseleaderrank"    TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="courseleaderrank_en" TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="courseleadercontact" TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="ou"                  TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="ou_en"               TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="website"             TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="lang"                TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="corriculumrole"      TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="corriculumrole_en"   TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="strong"              TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="weak"                TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="paralell"            TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="exclusive"           TYPE="text" NOTNULL="true"                       />
        <FIELD NAME="validity"             TYPE="int" LENGTH="10" NOTNULL="true"              />
        <FIELD NAME="validity_en"             TYPE="int" LENGTH="10" NOTNULL="true"              />
        <FIELD NAME="validby"             TYPE="int" LENGTH="10" NOTNULL="true"              />
        <FIELD NAME="validuntil"          TYPE="int" LENGTH="10" NOTNULL="true"              />
        <FIELD NAME="parent"              TYPE="int" LENGTH="10" NOTNULL="true"              />
        <FIELD NAME="locked"              TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"  />
        <FIELD NAME="draft"               TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"  />
        <FIELD NAME="published"           TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"  />
        <FIELD NAME="created_by"          TYPE="int" LENGTH="10" NOTNULL="true"              />
        <FIELD NAME="version"             TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="1"  />
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

